    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.3",
    "@types/react": "^16.9.53",
    "next": "^9.5.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
